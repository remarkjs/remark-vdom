{
  "name": "remark-vdom",
  "version": "1.0.1",
  "description": "Compile Markdown to VDOM with remark",
  "license": "MIT",
  "keywords": [
    "markdown",
    "html",
    "vdom",
    "virtual",
    "hyperscript",
    "dom",
    "stringify",
    "compile",
    "remark"
  ],
  "dependencies": {
    "collapse-white-space": "^1.0.0",
    "detab": "^1.0.0",
    "normalize-uri": "^1.0.0",
    "object-assign": "^4.0.1",
    "param-case": "^1.1.2",
    "property-information": "^2.0.0",
    "trim": "0.0.1",
    "trim-lines": "^1.0.0",
    "unist-util-visit": "^1.0.0",
    "virtual-dom": "^2.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wooorm/remark-vdom.git"
  },
  "bugs": "https://github.com/wooorm/remark-vdom/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js",
    "lib/"
  ],
  "devDependencies": {
    "browserify": "^13.0.0",
    "commonmark.json": "^0.21.0",
    "eslint": "^2.0.0",
    "esmangle": "^1.0.0",
    "istanbul": "^0.4.0",
    "jscs": "^2.0.0",
    "jscs-jsdoc": "^1.0.0",
    "remark": "^4.0.0-alpha.4",
    "remark-comment-config": "^3.0.0",
    "remark-github": "^4.0.1",
    "remark-license": "^2.0.0",
    "remark-lint": "^3.0.0",
    "remark-toc": "^3.0.0",
    "remark-usage": "^3.0.0",
    "remark-validate-links": "^3.0.0",
    "remark-yaml-config": "^3.0.0",
    "tape": "^4.0.0",
    "to-vfile": "^1.0.0",
    "vdom-to-html": "^2.2.0"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail",
    "build-bundle": "browserify index.js --bare -s remarkVDOM > remark-vdom.js",
    "build-mangle": "esmangle remark-vdom.js > remark-vdom.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint-api": "eslint .",
    "lint-style": "jscs --reporter inline .",
    "lint": "npm run lint-api && npm run lint-style",
    "test-api": "node test/index.js",
    "test-coverage": "istanbul cover test/index.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  }
}
